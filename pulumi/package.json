{
  "name": "pulumi",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "build": "esbuild --bundle --outfile=./dist/app.js --platform=node --target=node18 ./src/app.ts",
    "zip": "zip -j lambda.zip dist/app.js",
    "update": "aws lambda update-function-code --zip-file fileb://lambda.zip --function-name hello",
    "deploy": "run-s build zip update"
  },
  "dependencies": {
    "@pulumi/github": "^5.22.0",
    "@pulumi/pulumi": "^3.92.0",
    "hono": "^3.9.2"
  },
  "devDependencies": {
    "esbuild": "^0.17.11",
    "npm-run-all": "^4.1.5"
  }
}
